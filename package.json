{
  "name": "wipeout",
  "displayName": "wipeout",
  "description": "Wipeout Modding",
  "version": "0.0.6",
  "engines": {
    "vscode": "^1.71.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:wipeout.unpack.psarc",
    "onCommand:wipeout.unpack.wad",
    "onCustomEditor:wipeout.view.rcsmodel",
    "onCustomEditor:wipeout.view.vexx",
    "onCustomEditor:wipeout.view.gtf",
    "onCustomEditor:wipeout.view.dds"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "wipeout.unpack.psarc",
        "title": "Unpack PSARC"
      },
      {
        "command": "wipeout.unpack.wad",
        "title": "Unpack WAD"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "wipeout.unpack.psarc",
          "when": "resourceExtname == /\\.psarc/i",
          "group": "wipeout@1"
        },
        {
          "command": "wipeout.unpack.wad",
          "when": "resourceExtname =~ /\\.wad/i",
          "group": "wipeout@1"
        }
      ]
    },
    "customEditors": [
      {
        "viewType": "wipeout.view.vexx",
        "displayName": "VEXX Scene",
        "selector": [
          {
            "filenamePattern": "*.vex"
          }
        ],
        "priority": "default"
      },
      {
        "viewType": "wipeout.view.rcsmodel",
        "displayName": "RCS Model",
        "selector": [
          {
            "filenamePattern": "*.rcsmodel"
          }
        ],
        "priority": "default"
      },
      {
        "viewType": "wipeout.view.gtf",
        "displayName": "GTF Texture",
        "selector": [
          {
            "filenamePattern": "*.gtf"
          }
        ],
        "priority": "default"
      },
      {
        "viewType": "wipeout.view.dds",
        "displayName": "DDS Texture",
        "selector": [
          {
            "filenamePattern": "*.dds"
          }
        ],
        "priority": "default"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch --mode development --env esbuild",
    "package": "webpack --mode production --devtool hidden-source-map",
    "vsix": "vsce package",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@types/pako": "^2.0.0",
    "@types/three": "^0.144.0",
    "@types/vscode": "^1.71.0",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "@vscode/test-electron": "^2.1.5",
    "crypto-browserify": "^3.12.0",
    "esbuild-loader": "^2.20.0",
    "eslint": "^8.20.0",
    "fork-ts-checker-webpack-plugin": "^7.2.13",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "path-browserify": "^1.0.1",
    "stream-browserify": "^3.0.0",
    "ts-loader": "^9.3.1",
    "typescript": "^4.7.4",
    "vsce": "^2.11.0",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  },
  "dependencies": {
    "gl-matrix": "^3.4.3",
    "lil-gui": "^0.17.0",
    "pako": "^2.0.4",
    "three": "^0.144.0"
  }
}
